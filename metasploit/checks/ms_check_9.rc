# Automated vulnerability verification script.
# This script will check for the presence of multiple vulnerabilities on multiple hosts.
# WARNING: This file should only be used in a legal, controlled lab environment.

# ---- Global Defaults ----
setg VERBOSE true
setg LogLevel 1
setg HttpClientTimeout 8
setg ConnectTimeout 8
setg THREADS 4

# Routable listener settings (Docker bridge IP, LAN IP, or 0.0.0.0 for broad compatibility)
setg SRVHOST 0.0.0.0
setg LHOST 0.0.0.0

# Targets
setg RHOSTS file:/opt/metasploit-framework/ms_checks/targets.list

# ---- Module Checks ----

# Apache Commons Text RCE (Text4Shell)
use exploit/multi/http/apache_commons_text4shell
set PARAM q                 # required by module
# set TARGETURI /           # adjust if needed
# set RPORT 8080            # common default for webapps
# set SSL true              # enable if target uses HTTPS
check
back
sleep 1

# Gitea Git Hooks RCE
use exploit/multi/http/gitea_git_hooks_rce
# set TARGETURI /           # adjust if not default
# set RPORT 3000            # default for Gitea
# set SSL true              # enable if HTTPS
check
back
sleep 1

# FlexDotnetCMS Arbitrary ASP File Upload
use exploit/windows/http/flexdotnetcms_upload_exec
# set TARGETURI /           # adjust if needed
# set RPORT 80              # default
# set SSL true              # enable if HTTPS
check
back
sleep 1

# WordPress Really Simple SSL Plugin 2FA Bypass â†’ RCE
use exploit/multi/http/wp_reallysimplessl_2fa_bypass_rce
# set TARGETURI /           # adjust if needed
# set RPORT 443             # typical WordPress HTTPS
# set SSL true              # if HTTPS
check
back
sleep 1

# ManageEngine ADSelfService Plus CVE-2022-28810
use exploit/windows/http/manageengine_adselfservice_plus_cve_2022_28810
set USERNAME fake_user      # required creds
set PASSWORD fake_password  # required creds
# set RPORT 8888            # ManageEngine default
check
back
sleep 1

# Progress WS_FTP RCE (CVE-2023-40044)
use exploit/windows/http/ws_ftp_rce_cve_2023_40044
# set RPORT 443             # WS_FTP default HTTPS
# set SSL true              # if HTTPS
check
back
sleep 1

# Microsoft SharePoint SSI + ViewState RCE
use exploit/windows/http/sharepoint_ssi_viewstate
set DOMAIN fake.local       # required
set USERNAME fake_user      # required
set PASSWORD fake_password  # required
# set RPORT 443             # SharePoint HTTPS
# set SSL true
check
back
sleep 1

# ---- End ----
exit
